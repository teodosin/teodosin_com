		"@types/node": "^20.11.0",
		"@types/p5": "^1.7.6",
		"gh-pages": "^6.1.1",
		"mdsvex": "^0.11.0",
		"remark-attr": "^0.11.1",
		"remark-unwrap-images": "^4.0.0",
